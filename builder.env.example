# Builder.io Configuration Environment Variables
# Copy this file to .env and fill in your actual values

# Builder.io API Key - Get this from your Builder.io dashboard
BUILDER_API_KEY=your_builder_api_key_here

# Builder.io Space ID (if using spaces)
BUILDER_SPACE_ID=your_space_id_here

# Development environment
NODE_ENV=development

# Local development server
PORT=5000
HOST=0.0.0.0

# Database URL (already configured in Replit)
# DATABASE_URL=postgresql://...

# GitHub Copilot settings (configured via VS Code extension)
GITHUB_COPILOT_ENABLED=true

# SSH Remote development (configured via Replit SSH)
SSH_HOST=*.replit.dev
SSH_PORT=22
SSH_USER=runner

# Builder.io Preview Settings
BUILDER_PREVIEW_URL=https://preview.builder.io/
BUILDER_CONTENT_API_URL=https://cdn.builder.io/api/v1/